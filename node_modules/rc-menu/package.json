  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:react-component/menu.git"
  },
  "bugs": {
    "url": "http://github.com/react-component/menu/issues"
  },
  "licenses": "MIT",
  "config": {
    "port": 8010
  },
  "scripts": {
    "build": "rc-tools run build",
